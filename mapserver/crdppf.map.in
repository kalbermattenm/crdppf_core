MAP
NAME CRDPPF
STATUS ON
SIZE 400 300
UNITS METERS
TRANSPARENT on
MAXSIZE 4096
SYMBOLSET "crdppf.sym"
FONTSET fonts.txt

#CONFIG "MS_ERRORFILE" "/tmp/ms.log"
DEBUG 3
EXTENT 496748.9216 183451.7632 598251.0784 226548.2368

WEB
  IMAGEPATH "/tmp"
  METADATA
    "ows_title"                         "CRDPPF OWS server"
    "ows_enable_request"          "*"
    "wfs_encoding"                   "utf-8"
    "wms_title"                        "${vars:instanceid} WMS Server"
    "wms_onlineresource"          "http://${vars:host}/${vars:instanceid}/wmscrdppf"
    "wms_srs"                         "EPSG:21781" 
  END
END

IMAGETYPE png24
IMAGECOLOR 255 255 255
TRANSPARENT OFF

RESOLUTION 150

OUTPUTFORMAT
  NAME png24
  DRIVER "AGG/PNG"
  MIMETYPE "image/png"
  IMAGEMODE "RGB"
  EXTENSION "png"
END

OUTPUTFORMAT
  NAME jpeg
  DRIVER "AGG/JPEG"
  MIMETYPE "image/jpeg"
  EXTENSION "jpeg"
  FORMATOPTION QUALITY=88
END

PROJECTION
  "init=epsg:21781"   ##required
END

LEGEND
 STATUS ON        # Draw legend in the map
 KEYSIZE 20 15      # Define the size of the legend icon.
 LABEL            # Used only for embedded legend (in the map)
   TYPE TRUETYPE
   FONT "verdana"
   COLOR 0 0 0
   SIZE 8
   ANTIALIAS TRUE
 END
END


 # A scalebar object is defined one level below the map object.  This object 
  # controls how a scalebar is drawn by MapServer.  Scalebars can be embedded 
  # in the map itself or can be created as a separate image.  It has an 
  # associated MapServer CGI variable called "scalebar" (or [scalebar] when 
  # used in the HTML template).
  SCALEBAR
    IMAGECOLOR 255 255 255
    LABEL
      COLOR 0 0 0
      SIZE SMALL
    END
    STYLE 0
    SIZE 300 10
    COLOR 0 0 0
    UNITS METERS
    INTERVALS 5
    TRANSPARENT FALSE
    STATUS EMBED
  END # Scalebar object ends

LAYER
  NAME "parcelles"
  TYPE POLYGON
  CONNECTIONTYPE POSTGIS
  CONNECTION "user=${vars:dbuser} password=${vars:dbpassword} dbname=${vars:db} host=${vars:dbhost} port=${vars:dbport}"
  PROCESSING "CLOSE_CONNECTION=DEFER"
  DATA "geom from immeubles_canton using unique noobj using srid=21781"
  STATUS ON
  TEMPLATE "foo"
  CLASS
    NAME "name1"
    STYLE
      OUTLINECOLOR 120 120 0
    END 
  END
  METADATA
    "ows_title" "parcelles"
    "wms_srs" "epsg:21781"
    "wfs_enable_request" "*"
    "gml_types" "auto"
    "gml_include_items" "idemai,cadastre,nummai,typimm,srfmai,source,url_terris"
  END
  PROJECTION
    "init=epsg:21781"   ##required
  END
  MINSCALEDENOM 0
  MAXSCALEDENOM 10001
END

